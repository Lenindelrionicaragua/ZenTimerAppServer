{
  "name": "zentimerappserver",
  "version": "1.0.0",
  "description": "Backend for ZenTimerApp",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "setup": "npm install",
    "start": "node ./server",
    "dev": "nodemon ./server",
    "code-style-check": "npm run prettier && npm run lint",
    "pre-commit": "npm run code-style-check",
    "autofix": "npm run lint:fix && npm run prettier:fix",
    "lint": "eslint ./",
    "lint:fix": "eslint ./ --fix",
    "prettier": "prettier ./ --check",
    "prettier:fix": "prettier ./ --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lenindelrionicaragua/ZenTimerAppServer.git"
  },
  "author": "RDL",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Lenindelrionicaragua/ZenTimerAppServer/issues"
  },
  "homepage": "https://github.com/Lenindelrionicaragua/ZenTimerAppServer#readme",
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "babel-jest": "^27.5.1",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^8.0.3",
    "nock": "^13.5.4",
    "nodemon": "^3.1.7",
    "prettier": "^2.4.1",
    "prettier-eslint": "^13.0.0",
    "supertest": "^6.1.6"
  },
  "dependencies": {
    "axios": "^1.6.8",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "google-auth-library": "^9.10.0",
    "handlebars": "^4.7.8",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "mongoose": "^6.0.13",
    "nodemailer": "^6.9.13",
    "semver": "^7.6.3",
    "uuid": "^9.0.1"
  }
}
